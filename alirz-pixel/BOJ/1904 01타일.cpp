// 걸린시간 : 8분 (+ 버스 안에서 생각함)

#include <iostream>
#include <vector>
#define MOD 15746

using namespace std;

int main() {
	int n;
	cin >> n;

	vector<int> arr(n + 1);
	arr[0] = 1;
	arr[1] = 1;
	for (int i = 2; i <= n; i++) {
		arr[i] = (arr[i - 1] + arr[i - 2]) % MOD;
	}

	cout << arr[n];

	return 0;
}
